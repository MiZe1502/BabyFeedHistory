    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/date-fns": "^2.6.0",
    "apollo-server-express": "^2.21.0",
    "bcrypt": "^5.0.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "date-fns": "^2.19.0",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "uuid": "^8.3.2"
