  "bugs": {
    "url": "https://github.com/MiZe1502/BabyFeedHistory/issues"
  },
  "homepage": "https://github.com/MiZe1502/BabyFeedHistory#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.29",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "cross-env": "^7.0.3",
