{
  "name": "babyfeedhistory",
  "version": "1.0.0",
  "description": "Backend for app to store baby feeding history",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "start_dev": "tsc && NODE_ENV=development HOST=http://localhost node dist/index.js",
    "start_prod": "tsc && cross-env NODE_ENV=production node dist/index.js",
    "build_dev": "cross-env NODE_ENV=development tsc",
    "build_prod": "cross-env NODE_ENV=production tsc",
    "watch": "cross-env NODE_ENV=development HOST=http://localhost nodemon --exec ts-node -- ./src/index.ts",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiZe1502/BabyFeedHistory.git"
  },
